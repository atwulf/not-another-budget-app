{
  "name": "not-another-budget-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prestart": "npm run build:client",
    "start": "node ./bin/www",
    "start:server:watch": "nodemon --ignore src/ --ignore public/ ./bin/www",
    "start:server:debug": "node --inspect ./bin/www",
    "start:client": "parcel ./src/index.html -d ./public",
    "start:client:watch": "parcel watch ./src/index.html -d ./public",
    "prebuild:client": "rm ./public/*.{html,js,map}",
    "build:client": "parcel build ./src/index.html -d ./public",
    "db:migrate": "node ./config/migrate.js"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "express-error-handler": "^1.0.1",
    "express-handlebars": "^2.0.1",
    "hbs": "~3.1.0",
    "moment": "^2.19.3",
    "mongoose": "latest",
    "morgan": "~1.6.1",
    "node-sass-middleware": "0.8.0",
    "numeral": "^1.5.3",
    "pg": "^7.4.1",
    "ramda": "^0.25.0",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "sequelize": "^4.37.5",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "nodemon": "^1.17.3",
    "parcel-bundler": "^1.8.1"
  }
}
